{
    "metadata": [
        {
            "src": [
                {
                    "src": "../",
                    "files": [
                        "Assets/Scripts/Features/DOTS Dynamic Bone/*.cs",
                        "Assets/Scripts/Features/DOTS Dynamic Bone/Editor/*.cs"
                    ]
                }
            ],
            "filter": "filterConfig.yml",
            "dest": "api",
			"disableGitFeatures": false
        }
    ],
	"build": {
		"dest": "_site",
		"template": [ "default" ],
		"globalMetadata":
		{
			"_appTitle": "DOTS Dynamic Bone",
			"_appFooter": "DOTS Dynamic Bone Documentation",
			"_enableSearch": true
		},
		"sitemap":
		{
			"baseUrl": "https://github.com/Steven9Smith/DOTS-Dynamic-Bone" // The URL of your documentation website
		},
		"content": [
			{
				"files": [
					"api/**.yml",
					"api/index.md"
				]
			},
			{
				"files": [
					"manual/**.md",
					"manual/toc.yml",
					"toc.yml",
					"*.md"
				]
			}	,
			{
				"files": [
					"examples/**.md",
					"examples/toc.yml",
					"toc.yml",
					"*.md"
				]
			}	
		],
		"resource": [
			{
				"files": [
					"images/**"
				]
			}
		],
		"overwrite": [
			{
				"files": [
					"apidoc/**.md",
					"_site/**"
				]
			}
		],
		"globalMetadataFiles": [],
		"fileMetadataFiles": [],
		"postProcessors": [],
		"markdownEngineName": "dfm",
		"noLangKeyword": false,
		"keepFileLink": false,
		"cleanupCacheHistory": false,
		"disableGitFeatures": false
	}
}